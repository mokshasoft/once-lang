-- hello.once: Hello World for Once
--
-- Demonstrates:
--   - String literals with encoding (String Utf8)
--   - Primitives (println from linux File interpretation)
--   - Executable mode (main : IO Unit)
--
-- Build and run:
--   once build --exe --interp=linux examples/hello.once -o hello
--   ./hello

primitive println : Eff (String Utf8) Unit

main : IO Unit
main = compose println "Hello for Once"
